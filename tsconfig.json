{
	"extends": "./.wxt/tsconfig.json",
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"jsx": "react-jsx",
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"./src/*"
			],
			"@entrypoints/*": [
				"src/entrypoints/*"
			],
			"@components/*": [
				"./components/*"
			],
			"@dashboard/*": [
				"src/entrypoints/dashboard/*"
			]
		}
	}
}